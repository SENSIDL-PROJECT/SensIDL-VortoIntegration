<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated sensIDL -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="sensidl.presentation.SensidlModelWizardID"
            name="%_UI_SensidlModelWizard_label"
            class="sensidl.presentation.SensidlModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SensidlModelFile.gif">
         <description>%_UI_SensidlModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated sensIDL -->
      <editor
            id="sensidl.presentation.SensidlEditorID"
            name="%_UI_SensidlEditor_label"
            icon="icons/full/obj16/SensidlModelFile.gif"
            extensions="sensidl"
            class="sensidl.presentation.SensidlEditor"
            contributorClass="sensidl.presentation.SensidlActionBarContributor">
      </editor>
   </extension>

</plugin>
